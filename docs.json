{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Documentation",
  "colors": {
    "primary": "#bf5226",
    "light": "#bf5226",
    "dark": "#bf5226"
  },
  "favicon": "./favicon.png",
  "banner": {
    "content": "Welcome to the new MaxAI documentation. Alongside a comprehensive documentation update, we have launched video generation capabilities and enhanced our image generation models. Refer to the API documentation to begin exploring the platform.",
    "dismissible": false
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "introduction/dashboard", "introduction/authentication", "introduction/rate-limits", "introduction/changelogs"]
          }
        ]
      }, {
        "tab": "API reference",
        "groups": [
          {
            "group": "Images",
            "pages": ["api-reference/images/create-an-image-generation-request", "api-reference/images/poll-an-image-generation-request", "api-reference/images/cancel-an-image-generation-request"]
          }, {
            "group": "Videos",
            "pages": ["api-reference/videos/create-a-video-generation-request", "api-reference/videos/poll-a-video-generation-request", "api-reference/videos/cancel-a-video-generation-request"]
          }, {
            "group": "Utilities",
            "pages": ["api-reference/utilities/get-account-information", "api-reference/utilities/get-account-usage", "api-reference/utilities/get-api-keys", "api-reference/utilities/delete-an-api-key"]
          }
        ]
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "api": {
    "mdx": {
      "server": "https://api.maxai.lol/v1"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "API Status",
        "href": "https://status.maxai.lol"
      },
      {
        "label": "Support",
        "href": "mailto:support@maxai.lol"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.maxai.lol"
    }
  },
  "contextual": {
    "options": [
      "chatgpt",
      "claude",
      "copy",
      "perplexity"
    ]
  },
  "footer": {
    "socials": {
      "discord": "https://support.maxai.lol",
      "website": "https://status.maxai.lol"
    }
  },
  "errors": {
    "404": {
      "redirect": true
    }
  }
}